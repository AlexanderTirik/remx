    "test-watch": "jest --coverage --watch"
  },
  "dependencies": {
    "lodash": "4.x.x",
    "mobx": "2.x.x",
    "mobx-react": "3.x.x"
  },
  "devDependencies": {
    "react-native": "0.38.0",
    "react": "15.4.1",
    "react-test-renderer": "15.4.1",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-react-native": "1.x.x",
    "babel-register": "6.x.x",
    "babel-jest": "17.x.x",
    "eslint": "3.x.x",
    "eslint-plugin-babel": "4.x.x",
    "eslint-plugin-react": "6.x.x",
    "eslint-plugin-react-native": "2.x.x",
    "jest": "17.x.x",
    "jest-cli": "17.x.x"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testPathDirs": [
      "node_modules",
      "src"
    ],
    "resetMocks": true,
    "resetModules": true
  }
}
