{
  "name": "remx",
  "version": "0.1.23",
  "description": "Idiomatic mobx",
  "license": "MIT",
  "keywords": [
    "React",
    "Native",
    "redux",
    "mobx",
    "idiomatic"
  ],
  "author": "Daniel Zlotin <zlotindaniel@gmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/remx.git"
  },
  "bugs": {
    "url": "https://github.com/wix/remx/issues"
  },
  "homepage": "https://github.com/wix/remx",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel src --ignore test.js -d dist",
