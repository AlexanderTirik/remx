{
  "name": "remx",
  "version": "0.1.16",
  "description": "Idiomatic mobx",
  "license": "MIT",
  "keywords": [
    "React",
    "Native",
    "redux",
    "mobx",
    "idiomatic"
  ],
  "author": "Daniel Zlotin <zlotindaniel@gmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/remx.git"
  },
  "bugs": {
    "url": "https://github.com/wix/remx/issues"
  },
  "homepage": "https://github.com/wix/remx",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && babel src --ignore test.js -d dist",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "release": "npm version patch && npm publish && git push"
  },
  "dependencies": {
    "lodash": "4.x.x",
    "mobx": "2.x.x",
    "mobx-react": "4.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-react-native": "1.x.x",
    "babel-register": "6.x.x",
    "babel-jest": "17.x.x",
    "eslint": "3.x.x",
    "eslint-plugin-babel": "4.x.x",
    "eslint-plugin-react": "6.x.x",
    "eslint-plugin-react-native": "2.x.x",
    "jest": "17.x.x",
    "jest-cli": "17.x.x"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "testPathDirs": [
      "node_modules",
      "src"
    ],
    "resetMocks": true,
    "resetModules": true
  }
}
